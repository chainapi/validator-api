{
  "name": "validator-api",
  "version": "1.0.0",
  "description": "Validate Airnode config files over HTTP",
  "main": "index.js",
  "author": "ChainAPI <https://chainapi.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "clean": "rimraf -rf ./dist",
    "dev": "nodemon --ext '*' --watch src --exec 'ts-node src/index.ts'",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "postinstall": "husky install && yarn clean",
    "prettify": "prettier --write ./**/*.{js,ts,json,md}",
    "pretty-quick": "pretty-quick --staged --pattern \"**/*.*(js|ts|json|md)\"",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "validator-0.6": "npm:@api3/airnode-validator@0.6.5",
    "validator-0.7": "npm:@api3/airnode-validator@0.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
